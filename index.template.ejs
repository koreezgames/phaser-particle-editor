<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>
        <%= htmlWebpackPlugin.options.title %>
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <!--JQuery Min-->
    <script src="//code.jquery.com/jquery-3.2.1.slim.min.js"></script>
    <!--Bootstrap Min-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js"></script>
    <!--Bootstrap Colorpicker Min-->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-colorpicker/2.5.1/css/bootstrap-colorpicker.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-colorpicker/2.5.1/js/bootstrap-colorpicker.min.js"></script>
    <!--Font Awesome-->
    <script src="https://use.fontawesome.com/4f9103c56a.js"></script>
    <!--Local Resources-->
    <link rel="stylesheet" type="text/css" href="assets/css/style.css">
    <script src="index.js"></script>
</head>

<body>
    <div id="sandboxContainer" class="col-md-12 no-padding"></div>
    <div id="controlsContainer">
        <div class="container-fluid">
            <div class="col-md-12 no-padding">
                <!-- Main Buttons Group -->
                <div class="btn-group main-buttons" role="group">
                    <a type="button" class="btn btn-default" data-toggle="modal" data-target="#newProjectModal">
                        <i class="fa fa-file" aria-hidden="true"></i>
                        <span class="button-title">New</span>
                    </a>
                    <a type="button" class="btn btn-default downloadProject">
                        <i class="fa fa-save" aria-hidden="true"></i>
                        <span class="button-title">Save</span>
                    </a>
                    <a type="button" class="btn btn-default" id="downloadEmitters">
                        <i class="fa fa-download" aria-hidden="true"></i>
                        <span class="button-title">Download</span>
                    </a>
                </div>
            </div>
            <div class="row justify-content-center align-items-center">
                <input id="projectName" type="text" class="pureInput" oninput="this.style.width = ((this.value.length + 1) * 8) + 'px'">
            </div>
            <!-- Emitters Tabs -->
            <div class="emitterTabContainer">
                <ul id="emittersTabsGroup" class="nav nav-tabs" role="tablist">
                    <li id="addEmitterButtonLi" class="nav-item" data-toggle="modal" data-target="#createEmitterModal">
                        <a id="addEmitterButton" class="nav-link" data-toggle="tab" role="tab">
                            <i class="fa fa-plus" aria-hidden="true"></i>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- Emitter Options Accordion -->
            <div class="tab-content">
                <div class="tab-pane fade in active" id="emitterContent" role="tabpanel">
                    <div id="emitter-accordion" class="accordion" role="tablist" aria-multiselectable="true">
                        <div class="card">
                            <div class="card-header" role="tab" id="headingOne">
                                <p class="mb-0">
                                    <a data-toggle="collapse" data-parent="#emitter-accordion" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                                        Emitter Options
                                    </a>
                                </p>
                            </div>
                            <div id="collapseOne" class="collapse show" role="tabpanel" aria-labelledby="headingOne">
                                <div class="card-block">
                                    <div class="col-md-12">
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>OPTIONS</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="flow" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Flow</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="explode" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Explode</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-12 option">
                                                    <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                        <legend class="option-group-title">
                                                            <span>Lifespan</span>
                                                        </legend>
                                                        <div class="row">
                                                            <div class="col-md-6 option">
                                                                <div class="input-group input-group-sm">
                                                                    <span class="input-group-addon">Min</span>
                                                                    <input id="lifespanMin" type="number" class="form-control" placeholder="min" step="1">
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6 option">
                                                                <div class="input-group input-group-sm">
                                                                    <span class="input-group-addon">Max</span>
                                                                    <input id="lifespanMax" type="number" class="form-control" placeholder="max" step="1">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Frequency</span>
                                                        <input id="frequency" type="text" class="form-control" placeholder="Frequency">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Quantity</span>
                                                        <input id="quantity" type="text" class="form-control" placeholder="Quantity">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Total</span>
                                                        <input id="total" type="text" class="form-control" placeholder="Total">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Max Particles</span>
                                                        <input id="maxParticles" type="text" class="form-control" placeholder="Max particles">
                                                    </div>
                                                </div>
                                                <div class="col-md-12 option">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="immediate" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Immediate</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="collide" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Collide</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="collideWorldBounds" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Collide World Bounds</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>OFFSET</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">X</span>
                                                        <input id="offsetX" type="number" class="form-control" placeholder="OffsetX">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Y</span>
                                                        <input id="offsetY" type="number" class="form-control" placeholder="OffsetX">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>GRAVITY</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">X</span>
                                                        <input id="gravityX" type="number" class="form-control" placeholder="GravityX">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Y</span>
                                                        <input id="gravityY" type="number" class="form-control" placeholder="GravityY">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>BOUNCE</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">X</span>
                                                        <input id="bounceX" type="number" class="form-control" placeholder="BounceX">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Y</span>
                                                        <input id="bounceY" type="number" class="form-control" placeholder="BounceY">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>START ROTATION</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="startRotationStatus" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Enabled</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group  input-group-sm">
                                                        <span class="input-group-addon">Min</span>
                                                        <input id="startRotationMin" type="number" class="form-control" placeholder="min" min="-360" max="360" value="0">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group  input-group-sm">
                                                        <span class="input-group-addon">Max</span>
                                                        <input id="startRotationMax" type="number" class="form-control" placeholder="max" min="-360" max="360" value="0">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>ANCHOR</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="input-group">
                                                        <label class="custom-control custom-checkbox">
                                                            <input id="anchorStatus" type="checkbox" class="custom-control-input">
                                                            <span class="custom-control-indicator"></span>
                                                            <span class="custom-control-description">Enabled</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group  input-group-sm">
                                                        <span class="input-group-addon">X</span>
                                                        <input id="anchorX" type="number" class="form-control" placeholder="min" step="0.1" min="0" max="1" value="0.5">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group  input-group-sm">
                                                        <span class="input-group-addon">Y</span>
                                                        <input id="anchorY" type="number" class="form-control" placeholder="max" step="0.1" min="0" max="1" value="0.5">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>HEIGHT & WIDTH</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Width</span>
                                                        <input id="width" type="number" class="form-control" placeholder="width">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Height</span>
                                                        <input id="height" type="number" class="form-control" placeholder="height">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>SCALE</span>
                                            </legend>
                                            <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                <legend class="option-group-title">
                                                    <span>Specific</span>
                                                </legend>
                                                <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                    <legend class="option-group-title">
                                                        <span>X</span>
                                                    </legend>
                                                    <div class="row">
                                                        <div class="col-md-6 option">
                                                            <div class="input-group  input-group-sm">
                                                                <span class="input-group-addon">From</span>
                                                                <input id="scaleFromX" type="number" class="form-control" placeholder="X" step="0.1">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 option">
                                                            <div class="input-group input-group-sm">
                                                                <span class="input-group-addon">To</span>
                                                                <input id="scaleToX" type="number" class="form-control" placeholder="X" step="0.1">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                                <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                    <legend class="option-group-title">
                                                        <span>Y</span>
                                                    </legend>
                                                    <div class="row">
                                                        <div class="col-md-6 option">
                                                            <div class="input-group input-group-sm">
                                                                <span class="input-group-addon">From</span>
                                                                <input id="scaleFromY" type="number" class="form-control" placeholder="Y" step="0.1">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 option">
                                                            <div class="input-group input-group-sm">
                                                                <span class="input-group-addon">To</span>
                                                                <input id="scaleToY" type="number" class="form-control" placeholder="Y" step="0.1">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                                <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                    <legend class="option-group-title">
                                                        <span>Easing</span>
                                                    </legend>
                                                    <div class="row">
                                                        <div class="col-md-6 option">
                                                            <div class="dropdown" id="scaleEasingDropdown">
                                                                <button class="btn btn-sm dropdown-toggle form-control" id="scaleEasing" data-toggle="dropdown" aria-expanded="false">Easing
                                                                </button>
                                                                <div class="dropdown-menu" aria-labelledby="scaleEasing">
                                                                    <a class="dropdown-item scaleEase">Back</a>
                                                                    <a class="dropdown-item scaleEase">Bounce</a>
                                                                    <a class="dropdown-item scaleEase">Circular</a>
                                                                    <a class="dropdown-item scaleEase">Cubic</a>
                                                                    <a class="dropdown-item scaleEase">Elastic</a>
                                                                    <a class="dropdown-item scaleEase">Exponential</a>
                                                                    <a class="dropdown-item scaleEase">Linear</a>
                                                                    <a class="dropdown-item scaleEase">Quadratic</a>
                                                                    <a class="dropdown-item scaleEase">Quartic</a>
                                                                    <a class="dropdown-item scaleEase">Quintic</a>
                                                                    <a class="dropdown-item scaleEase">Sinusoidal</a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 option" id="scaleEasingModeDropdown">
                                                            <div class="dropdown">
                                                                <button class="btn btn-sm dropdown-toggle form-control" id="scaleEasingMode" data-toggle="dropdown" aria-expanded="false">Mode
                                                                </button>
                                                                <div class="dropdown-menu" aria-labelledby="scaleEasingMode">
                                                                    <a class="dropdown-item scaleEaseMode">In</a>
                                                                    <a class="dropdown-item scaleEaseMode">Out</a>
                                                                    <a class="dropdown-item scaleEaseMode">InOut</a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 option">
                                                            <div class="input-group input-group-sm">
                                                                <span class="input-group-addon">Rate</span>
                                                                <input id="scaleRate" type="number" class="form-control" placeholder="rate">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-5 option">
                                                            <div class="col-md-2"></div>
                                                            <label class="custom-control custom-checkbox">
                                                                <input id="scaleYoyo" type="checkbox" class="custom-control-input">
                                                                <span class="custom-control-indicator"></span>
                                                                <span class="custom-control-description">Yoyo</span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </fieldset>
                                            <div class="col-md-12 option">
                                                <div class="input-group">
                                                    <label class="custom-control custom-checkbox">
                                                        <input id="randomScale" type="checkbox" class="custom-control-input">
                                                        <span class="custom-control-indicator"></span>
                                                        <span class="custom-control-description">Random</span>
                                                    </label>
                                                </div>
                                            </div>
                                            <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                <legend class="option-group-title">Random</legend>
                                                <div class="row">
                                                    <div class="col-md-6 option">
                                                        <div class="input-group input-group-sm">
                                                            <span class="input-group-addon">Min</span>
                                                            <input id="minScale" type="number" class="form-control" placeholder="min" step="0.1">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 option">
                                                        <div class="input-group  input-group-sm">
                                                            <span class="input-group-addon">Max</span>
                                                            <input id="maxScale" type="number" class="form-control" placeholder="max" step="0.1">
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>ROTATION</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Min</span>
                                                        <input id="rotationMin" type="number" class="form-control" placeholder="min">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Max</span>
                                                        <input id="rotationMax" type="number" class="form-control" placeholder="max">
                                                    </div>
                                                </div>
                                                <div class="col-md-12 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Angular Drag</span>
                                                        <input id="angularDrag" type="number" class="form-control" placeholder="Angular Drag">
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>SPEED</span>
                                            </legend>
                                            <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                <legend class="option-group-title">
                                                    <span>X</span>
                                                </legend>
                                                <div class="row">
                                                    <div class="col-md-6 option">
                                                        <div class="input-group  input-group-sm">
                                                            <span class="input-group-addon">Min</span>
                                                            <input id="minSpeedX" type="number" class="form-control" placeholder="min">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 option">
                                                        <div class="input-group  input-group-sm">
                                                            <span class="input-group-addon">Max</span>
                                                            <input id="maxSpeedX" type="number" class="form-control" placeholder="max">
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                <legend class="option-group-title">
                                                    <span>Y</span>
                                                </legend>
                                                <div class="row">
                                                    <div class="col-md-6 option">
                                                        <div class="input-group  input-group-sm">
                                                            <span class="input-group-addon">Min</span>
                                                            <input id="minSpeedY" type="number" class="form-control" placeholder="min">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 option">
                                                        <div class="input-group input-group-sm">
                                                            <span class="input-group-addon">Max</span>
                                                            <input id="maxSpeedY" type="number" class="form-control" placeholder="max">
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </fieldset>
                                        <fieldset class="option-group-fieldset">
                                            <legend class="option-group-title">
                                                <span>ALPHA</span>
                                            </legend>
                                            <div class="row">
                                                <div class="col-md-6 option">
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-addon">Min</span>
                                                        <input id="alphaMin" type="number" class="form-control" placeholder="alpha">
                                                    </div>
                                                </div>
                                                <div class="col-md-6 option">
                                                    <div class="input-group  input-group-sm">
                                                        <span class="input-group-addon">Max</span>
                                                        <input id="alphaMax" type="number" class="form-control" placeholder="alpha">
                                                    </div>
                                                </div>
                                            </div>
                                            <fieldset class="option-group-fieldset option-group-nested-fieldset">
                                                <legend class="option-group-title">
                                                    <span>Easing</span>
                                                </legend>
                                                <div class="row">
                                                    <div class="col-md-6 option">
                                                        <div class="dropdown" id="alphaEasingDropdown">
                                                            <button class="btn btn-sm dropdown-toggle form-control" id="alphaEasing" data-toggle="dropdown" aria-expanded="false">Easing
                                                            </button>
                                                            <div class="dropdown-menu" aria-labelledby="alphaEasing">
                                                                <a class="dropdown-item">Back</a>
                                                                <a class="dropdown-item">Bounce</a>
                                                                <a class="dropdown-item">Circular</a>
                                                                <a class="dropdown-item">Cubic</a>
                                                                <a class="dropdown-item">Elastic</a>
                                                                <a class="dropdown-item">Exponential</a>
                                                                <a class="dropdown-item">Linear</a>
                                                                <a class="dropdown-item">Quadratic</a>
                                                                <a class="dropdown-item">Quartic</a>
                                                                <a class="dropdown-item">Quintic</a>
                                                                <a class="dropdown-item">Sinusoidal</a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 option">
